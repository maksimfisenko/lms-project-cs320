<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="480.0" maxWidth="640.0" minHeight="480.0" minWidth="640.0" prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.control.AdminSignUpController">
   <children>
      <Label layoutX="269.0" layoutY="22.0" prefHeight="27.0" prefWidth="102.0" text="New Admin">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="firstNameField" layoutX="245.0" layoutY="99.0" promptText="First Name" />
      <TextField fx:id="lastNameField" layoutX="246.0" layoutY="132.0" promptText="Last Name" />
      <TextField fx:id="loginField" layoutX="245.0" layoutY="166.0" promptText="Login" />
      <PasswordField fx:id="passwordField" layoutX="246.0" layoutY="198.0" promptText="Password" />
      <PasswordField fx:id="passwordCheckField" layoutX="246.0" layoutY="233.0" promptText="Confirm Password" />
      <Button fx:id="signUpButton" layoutX="289.0" layoutY="266.0" mnemonicParsing="false" onAction="#save" text="Sign Up" />
      <Button fx:id="backButton" layoutY="8.0" mnemonicParsing="false" onAction="#back" prefHeight="54.0" prefWidth="66.0" style="-fx-background-color: transparent;" text="←">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
